              description="OLE DB Source"
              name="OLE DB NorthWind"
              usesDispositions="true"
              version="7">
              <properties>
                <property
                  dataType="System.Int32"
                  description="The number of seconds before a command times out.  A value of 0 indicates an infinite time-out."
                  name="CommandTimeout">0</property>
                <property
                  dataType="System.String"
                  description="Specifies the name of the database object used to open a rowset."
                  name="OpenRowset"></property>
                <property
                  dataType="System.String"
                  description="Specifies the variable that contains the name of the database object used to open a rowset."
                  name="OpenRowsetVariable"></property>
                <property
                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">WITH ProductSalesNW AS (
  SELECT 
    p.ProductID,
    COUNT(DISTINCT o.OrderID) as TotalOrdersNW,
    SUM(od.UnitPrice * od.Quantity) as TotalRevenueNW
  FROM 
    [Orders] o
    INNER JOIN [Order Details] od ON o.OrderID = od.OrderID
    INNER JOIN [Products] p ON od.ProductID = p.ProductID
  GROUP BY 
    p.ProductID
)

SELECT 
  psnw.ProductID,
  psnw.TotalOrdersNW,
  psnw.TotalRevenueNW,
  COUNT(DISTINCT e.EmployeeID) as TotalSalespeopleNW
FROM 
  ProductSalesNW psnw
  INNER JOIN [Order Details] od ON psnw.ProductID = od.ProductID
  INNER JOIN [Orders] o ON od.OrderID = o.OrderID
  INNER JOIN [Employees] e ON o.EmployeeID = e.EmployeeID -- Suponiendo que EmployeeID en Orders es el vendedor
GROUP BY 
  psnw.ProductID, psnw.TotalOrdersNW, psnw.TotalRevenueNW;</property>
