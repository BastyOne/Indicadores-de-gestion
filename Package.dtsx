                  dataType="System.String"
                  description="The SQL command to be executed."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    P.ProductID,
    P.StandardCost AS CostoPorUnidad,
    P.ListPrice AS PrecioPorUnidad,
    SOH.Quantity AS CantidadEnInventario,
    SOH.Quantity + SUM(SOD.OrderQty) AS InventarioInicial, 
    SUM(SOD.OrderQty) AS UnidadesVendidas,
    SUM(SOD.LineTotal) AS TotalVentas
FROM 
    Production.Product P
JOIN 
    Production.ProductInventory SOH ON P.ProductID = SOH.ProductID
JOIN 
    Sales.SalesOrderDetail SOD ON P.ProductID = SOD.ProductID
WHERE 
    P.StandardCost IS NOT NULL AND
    SOH.Quantity IS NOT NULL
GROUP BY 
    P.ProductID, P.StandardCost, P.ListPrice, SOH.Quantity;</property>
